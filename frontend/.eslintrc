{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "rules": {
      "comma-dangle": ["error", "never"],
      "no-underscore-dangle": 0,
      "no-console": "warn",
      "no-unused-expressions": ["error", {"allowTernary": true}],
      "no-return-assign": [2, "except-parens"],
      "no-param-reassign": ["error", { "props": false }]
    }
  }
